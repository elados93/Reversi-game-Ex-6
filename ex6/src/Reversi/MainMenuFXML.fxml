<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Reversi.MainMenuController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox prefHeight="395.0" prefWidth="362.0" spacing="50.0">
      			<children>
      				<Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="71.0" prefWidth="362.0" text="Reversi" textAlignment="CENTER">
      					<font>
      						<Font name="Purisa Bold" size="51.0" />
      					</font>
      				</Label>
      				<Button fx:id="Start_Game" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#startGame" text="Start Game" textAlignment="CENTER">
      					<VBox.margin>
      						<Insets left="122.0" top="15.0" />
      					</VBox.margin>
      					<opaqueInsets>
      						<Insets />
      					</opaqueInsets>
      				</Button>
      				<Button fx:id="Settings" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#settings" text="Settings" textAlignment="CENTER">
      					<VBox.margin>
      						<Insets left="130.0" />
      					</VBox.margin>
      					<opaqueInsets>
      						<Insets />
      					</opaqueInsets>
      				</Button>
      				<Button fx:id="Exit" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#exitGame" text="Exit" textAlignment="CENTER">
      					<VBox.margin>
      						<Insets left="140.0" />
      					</VBox.margin>
      					<opaqueInsets>
      						<Insets />
      					</opaqueInsets>
      				</Button>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Created by Sheher &amp; Eled">
               <font>
                  <Font name="Purisa" size="13.0" />
               </font>
            </Text>
      			</children>
      		</VBox>
   </children>
</GridPane>
